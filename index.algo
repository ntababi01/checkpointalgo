ALGORITHM LongestWordLength
VAR
i,p,v:INTEGER;
ch:string;
BEGIN
 i := 0;
 p :=0;
 v:=0;
 print('donner un chainer de caractere')
 read(ch);
DO {
 IF (ch[i] <> ' ')THEN
         i:=i+1;
 IF (ch[i]in['o','a','i','u','y'])THEN
   v:=v+1;
  else
     p :=p+1;
     END_IF
 WHILE (ch[i]=('.'))
 END_WHILE            
    }
    write(ch);
}
 